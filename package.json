{
  "name": "brpoc",
  "version": "0.0.1",
  "description": "BR work for POC",
  "main": "dist/index.js",
  "author": "HCL",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env webpack --colors --progress",
    "build:prod": "cross-env NODE_ENV=production npm run build -- -p --colors --progress",
    "clean": "rimraf dist/",
    "install:nodeversion": "node --version",
    "install:npmversion": "npm --version",
    "lint": "eslint ./ --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "prebuild": "npm run clean",
    "preinstall": "npm run install:nodeversion && npm run install:npmversion"
  },
  "dependencies": {
    "lodash": "4.17.4",
    "moment": "2.20.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-highcharts": "16.0.2"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "clean-webpack-plugin": "0.1.18",
    "copy-webpack-plugin": "4.3.1",
    "cross-env": "5.1.3",
    "css-loader": "0.28.9",
    "eslint": "4.16.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.6.1",
    "extract-text-webpack-plugin": "3.0.2",
    "node-sass": "^4.7.2",
    "postcss-loader": "0.9.1",
    "react-hot-loader": "3.0.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.20.1",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0",
    "write-file-webpack-plugin": "4.2.0"
  },
  "config": {
    "cssModulePattern": "br-[name]__[local]"
  }
}
